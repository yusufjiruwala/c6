	<queryString>
		<![CDATA[select decode((select ord_type from order1 o where o.ord_code=106 and o.ord_no=order1.ord_reference),2,lg_info.lg_s_mbl,3,lg_info.LG_A_MAWB) bl,     order1.*,(select max(ord_fc_descr) from order2 where ord_code=141 and ord_no=order1.ord_no) max_currency,
(select oname from order1 ox where ord_code=106 and ox.ord_no=order1.ord_reference) job_oname,
(select max(FC_PRICE*ORD_ALLQTY) from order2 where ord_code=141 and ord_no=order1.ord_no) FC_AMOUNT ,lg_info.*,f_words_2((select max(FC_PRICE*ORD_ALLQTY) from order2 where ord_code=141 and ord_no=order1.ord_no),(select max(ord_fc_descr) from order2 where ord_code=141 and ord_no=order1.ord_no)) amt_in_word,to_char(sysdate,'dd/mm/rrrr hh24:mi') to_day_dat ,lcno,
y.parentcustomer parent_code,(select max(name) from c_ycust where code=y.parentcustomer) parent_name,
(select max(payterm) from order1 o where ord_code=106 and ord_no=order1.ord_reference) payterm_1,
(select max(attn) from order1 o where ord_code=106 and ord_no=order1.ord_reference) attn_1,
(select max(ORD_SHIP) from order1 o where ord_code=106 and ord_no=order1.ord_reference) CUST_REF,y.addr,y.tel,y.reference,y.email,ORDER1.ONAME,
(select bank_details from currency_master where code=(select max(ord_fc_descr) from order2 where order2.ord_no=order1.ord_no and ord_code=141)) bnk,
order1.ord_txt_wo,order1.ord_txt_iid
from order1,lg_info,c_ycust y
where order1.ord_code=141 and order1.ORD_no IN (select field1 from temporary where USERNM=$P{SES_ID} AND IDNO=141)
and y.code=order1.ord_ref
and lg_info.ord_no(+)=order1.ord_reference
order by order1.ord_no]]>
	</queryString>
